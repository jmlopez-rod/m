[MESSAGES CONTROL]
init-hook="import sys; sys.path.append('/workspaces/m/packages/python')"

disable=
  invalid-name,
  too-few-public-methods,
  wrong-import-order,
  redefined-builtin,
  missing-class-docstring,
  unexpected-keyword-arg,
  import-outside-toplevel,
  missing-module-docstring,
  ungrouped-imports,
  broad-except

extension-pkg-whitelist=pydantic

no-docstring-rgx=^_|run|add_parser|main|add_arguments|^test|(.*)Test$

good-names=_,
  i, j, k,
  x, y, z,
  ex, pr, fp

class-const-naming-style=snake_case

max-args=7


min-similarity-lines=10
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=no
ignore-signatures=yes
